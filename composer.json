{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "fideloper/proxy": "~4.0",
        "invisnik/laravel-steam-auth": "^3.2",
        "laravel/framework": "5.6.*",
        "laravel/nova": "*",
        "laravel/passport": "^6.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "restcord/restcord": "^0.3.0",
        "sbine/route-viewer": "^0.0.2",
        "socialiteproviders/discord": "^2.0",
        "spatie/laravel-medialibrary": "7.0.0",
        "zanysoft/laravel-zip": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/socialite"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan --no-ansi key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan --no-ansi package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
