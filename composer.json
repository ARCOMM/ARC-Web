{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url":
                "https://github.com/maksimru/laravel-google-cloud-storage.git"
        },
        {
            "type": "vcs",
            "url":
                "https://github.com/maksimru/flysystem-google-cloud-storage.git"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "spatie/laravel-medialibrary": "^6.6",
        "syntax/steam-api": "^2.0",
        "invisnik/laravel-steam-auth": "^2.3",
        "alaouy/youtube": "dev-master",
        "laravel/tinker": "^1.0",
        "chumper/zipper": "1.0.x",
        "jameslkingsley/laravel-mentions": "dev-master",
        "laravel-notification-channels/discord": "^0.0.1",
        "team-reflex/discord-php": "^4.0",
        "sentry/sentry-laravel": "^0.7.0",
        "superbalist/laravel-google-cloud-storage":
            "dev-master#3cb952fd4599f31bacdf36379703e85dbd0f6157",
        "superbalist/flysystem-google-storage":
            "dev-master#093b7e4b46f37720109e9efa594782cd0b3ced02",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": ["database", "app/Http/Controllers"],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/TestCase.php", "app/Http/Controllers"]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": ["php artisan key:generate"],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
